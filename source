            })
        end
    }
})

A.Button({
    Text = "Respawn",
    Callback = function()
       game.Players.LocalPlayer.Character.Humanoid.Health = 0
    end,
    Menu = {
       Information = function(self)
            UI.Banner({
                Text = "It respawns you, if it doesn't work. Please rejoin (Respawn doesn't work on God Mode)"            
            })
        end
    }
})

