local guiService = game:GetService("StarterGui")
local exploit = (syn_secure_call and "Synapse X") or (PROTOSMASHER_LOADED and "ProtoSmasher") or (SENTINEL_LOADED and "Sentinel") or (fireclient and "Calamari") or (is_sirhurt_closure and "SirHurt") or (KRNL_LOADED and "KRNL") or (shadow_env and "Shadow") or (IS_VIVA_LOADED and "VIVA") or (isvm and "Proxo") or (IS_COCO_LOADED and "COCO") or (unit and "UNIT.FUN")-- or (jit and "EasyExploits")

wait()

guiService:SetCore("SendNotification", {

Title = "Checking exploit!";
Text = "We are checking what exploit are you using";
Duration = 6;
})

wait(6)

guiService:SetCore("SendNotification", {

Title = "Almost done!";
Text = "If it doesn't tell you what exploit you using, you are using unknown function";
Duration = 6;
})

wait(5)

guiService:SetCore("SendNotification", {

Title = "Exploit detected!";
Text = "You are using ".. exploit;
Duration = 5;
})
